{
    "manifest_version": 3,
    "name": "LinkedIn Feed Blocker",
    "version": "1.0",
    "description": "Blocks the LinkedIn homepage feed",
    "permissions": [],
    "host_permissions": ["https://www.linkedin.com/feed/*"],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/feed/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  